<?xml version="1.0" encoding="UTF-8"?>
<rules>

    <configs>
        <config>
            <!-- 市场名称 -->
            <store>oppo</store>
            <!-- 执行次数 -->
            <runcount>10</runcount>

            <!-- 关键词 -->
            <keyword>定位</keyword>

            <!-- 包名 -->
            <packagename>com.jlzb.android</packagename>
        </config>
        <config>
            <!-- 市场名称 -->
            <store>oppo</store>
            <!-- 执行次数 -->
            <runcount>8</runcount>

            <!-- 关键词 -->
            <keyword>手机定位</keyword>

            <!-- 包名 -->
            <packagename>com.jlzb.android</packagename>
        </config>
        <config>
            <!-- 市场名称 -->
            <store>vivo</store>
            <!-- 执行次数 -->
            <runcount>8</runcount>

            <!-- 关键词 -->
            <keyword>定位</keyword>

            <!-- 包名 -->
            <packagename>com.jlzb.android</packagename>
        </config>
    </configs>

    <!-- 每次执行完休息时间 单位ms -->
    <sleeptime>2000</sleeptime>

    <!-- 是否执行拨号程序 -->
    <ischangeip>
        <!-- 开关 1打开 0关闭 -->
        <adslswitch>0</adslswitch>
        <connectname>宽带连接</connectname>
        <adsluser>051916392997</adsluser>
        <adslpass>696933</adslpass>
        <!-- 自动断线间隔时间 也就是第一次断线失败 和第二次尝试断线的间隔时间 -->
        <intervaltime>3000</intervaltime>
    </ischangeip>
</rules>